doctype html
html
  head
    title Admin - CEP MOAS
    != css('admin')
    
  body
    .container
      form.form-signin(method='post')
        h2 Please sign in
        input.form-control(type='email' placeholder='Email address' required autofocus)
        input.form-control(type='password' placeholder='Password' required autofocus)
        button.btn.btn-lg.btn-primary.btn-block Sign in
